{
  "name": "tfjs-core-wrapper",
  "license": "MIT",
  "scripts": {
    "watch": "parcel index.html --no-hmr --port 5050",
    "build": "parcel build index.html  --no-minify --public-url ./",
    "postinstall": "yarn upgrade --pattern @tensorflow",
    "link-local": "yalc link",
    "unlink-local": "yalc remove"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^0.14.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-polyfill": "~6.26.0",
    "babel-preset-env": "~1.6.1",
    "clang-format": "~1.2.2",
    "cross-env": "^5.1.6",
    "parcel-bundler": "^1.8.1",
    "parcel-plugin-typescript": "^1.0.0",
    "src": "^1.1.2",
    "typescript": "2.9.2",
    "utf8": "^3.0.0"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.3"
  }
}
